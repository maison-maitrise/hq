---
interface Props {
    title: string;
}

const {title} = Astro.props
---

<head>
  <meta charset="utf-8" />
  <link rel="icon" type="image/svg+xml" href="/favicon.png" />
  <meta name="viewport" content="width=device-width" />
  <meta name="theme-color" content="#F00000" />
  <title>{title}</title>
</head>
